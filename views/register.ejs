		<section id="register_div">
 			<form action="/register" autocomplete="on" method="POST">
  			<h1><p><i class="fa fa-lg fa-fw fa-user"></i>SIGN UP</p></h1>
        <div class="form-group">
          <label>Email</label>
          <input name="email" id="email" type="email" placeholder="Email" />
        </div>
  			<div class="form-group">
  				<label>Username</label>
  				<input name="username" id="username" type="text" placeholder="Username" />
  			</div>
  			<div class="form-group">
  				<label>Phone No</label>
  				<input name="phoneno" id="phone" type="text" placeholder="Phone no" onsubmit="validatePhone();" />
  			</div>
  			<div class="form-group">
  				<label>Password</label>
  				<input name="password" type="password" placeholder="password"/>
  			</div>
  			<div class="form-group">
  				<label>Confirm Password</label>
  				<input name="password_confirm" type="password" placeholder="password"/>
  			</div>
  			<div class="form-group">
  				<input type="submit" value="SIGN UP"/>
  			</div>
  			<div class="change_link" form-group"">Already a Member?
          <a href="/login"><input type="button" value="Log in"></a>
        </div>
 			</form>
		</section>
      </div><!--login box ended-->
